{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Electron (Windows)",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}\\node_modules\\.bin\\electron.cmd",
      "args": ["."],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "ELECTRON_ENABLE_LOGGING": "true"
      }
    }
  ]
}
